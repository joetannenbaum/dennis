{
    "name": "joetannenbaum/dennis",
    "description": "A CLI app to manage your DNS records.",
    "keywords": [
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "dns"
    ],
    "homepage": "https://github.com/joetannenbaum/dennis",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joetannenbaum/dennis/issues",
        "source": "https://github.com/joetannenbaum/dennis"
    },
    "authors": [
        {
            "name": "Joe Tannenbaum",
            "email": "joe@joe.codes"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "illuminate/validation": "^10.22",
        "laravel-zero/framework": "^10.0.2",
        "laravel/prompts": "^0.1.7",
        "nunomaduro/termwind": "^1.15.1",
        "spatie/laravel-data": "^3.8"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5",
        "spatie/laravel-ray": "^1.33",
        "tightenco/duster": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Validation/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "dennis"
    ]
}
